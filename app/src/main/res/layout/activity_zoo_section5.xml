<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/zooActivity_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/zoo_background5"
    android:orientation="horizontal"
    tools:context=".ZooSection5Activity">

    <ImageView
        android:id="@+id/rattlesnake_grey"
        android:layout_width="237dp"
        android:layout_height="118dp"
        android:layout_marginBottom="32dp"
        android:layout_marginEnd="330dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="209dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rattlesnake_grey" />

    <ImageView
        android:id="@+id/rattlesnake_color"
        android:layout_width="237dp"
        android:layout_height="118dp"
        android:layout_marginBottom="32dp"
        android:layout_marginEnd="330dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="209dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rattlesnake_color" />

    <ImageView
        android:id="@+id/alligator_grey"
        android:layout_width="243dp"
        android:layout_height="160dp"
        android:layout_marginBottom="33dp"
        android:layout_marginEnd="324dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="16dp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/rattlesnake_grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/alligator_grey" />

    <ImageView
        android:id="@+id/alligator_color"
        android:layout_width="243dp"
        android:layout_height="160dp"
        android:layout_marginBottom="33dp"
        android:layout_marginEnd="324dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="16dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/rattlesnake_grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/alligator_color" />

    <ImageView
        android:id="@+id/turtle_grey"
        android:layout_width="234dp"
        android:layout_height="125dp"
        android:layout_marginBottom="218dp"
        android:layout_marginEnd="46dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="16dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/alligator_grey"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/turtle_grey" />

    <ImageView
        android:id="@+id/turtle_color"
        android:layout_width="234dp"
        android:layout_height="125dp"
        android:layout_marginBottom="218dp"
        android:layout_marginEnd="46dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="16dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/alligator_grey"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/turtle_color" />

    <ImageView
        android:id="@+id/iguana_grey"
        android:layout_width="260dp"
        android:layout_height="183dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="6dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="19dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/alligator_grey"
        app:layout_constraintTop_toBottomOf="@+id/turtle_grey"
        app:srcCompat="@drawable/iguana_grey" />

    <ImageView
        android:id="@+id/iguana_color"
        android:layout_width="260dp"
        android:layout_height="183dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="6dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="19dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/alligator_grey"
        app:layout_constraintTop_toBottomOf="@+id/turtle_grey"
        app:srcCompat="@drawable/iguana_color" />
</android.support.constraint.ConstraintLayout>
